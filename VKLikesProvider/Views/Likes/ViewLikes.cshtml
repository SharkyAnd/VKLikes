@model IEnumerable<VKLikesProvider.Models.VKLike>

@{
    ViewBag.Title = "ViewLikes";
}

<h2>ViewLikes</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.PageId)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Moment)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.PageId)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Moment)
        </td>
    </tr>
}

</table>
